VERSION = 1.0

CC = gcc
CFLAGS = -Wall -ansi -pedantic -O2

OBJS = XbEvent.o XbText.o XbCursor.o SelectInputTree.o

# location of the X and motif header files
X_INCLUDE = -I/usr/openwin/include
XM_INCLUDE = -I/usr/dt/include

INCLUDE = $(X_INCLUDE) $(XM_INCLUDE)

all: objs lib

lib: libXb.a

libXb.a: $(OBJS)
	$(AR) $(ARFLAGS) $@ $?

objs: $(OBJS) 

%.o: %.c
	$(CC) -c $< $(CFLAGS) $(INCLUDE) -o $@

dist:
	rm -f Makefile config.cache config.log config.status.
	mkdir ../Xb-$(VERSION) ; cp -r * ../Xb-$(VERSION)
	cd ../ ; tar cvf - ./Xb-$(VERSION) | gzip > Xb-$(VERSION).tar.gz
tar:
	tar cvf - * | gzip > libXba.tar.gz

clean:
	rm -f core `find . -name '*.[oas]' -print`
	rm -f core `find . -name '*~' -print`

