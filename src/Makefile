###########################
#
# Targets *(should not need to edit anything below this line)
#
###########################

objs: $(OBJS)

all:
	cd .. ; make
%.o: %.c
	$(CC) -c $< $(CFLAGS) $(DEFS) $(ALL_INCLUDES) -o $@

tidy:
	rm -f core `find . -name '*.[as]' -print`
	rm -f core `find . -name 'core' -print`
	rm -f core `find . -name '*~' -print`
	rm -f xqs a.out

clean: tidy
	rm -f core `find . -name '*.[oas]' -print`
