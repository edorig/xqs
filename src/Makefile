OBJS = main.o WindowTab.o DisplayTab.o KeyboardTab.o PropertiesTab.o	\
	AccessTab.o ClientsTab.o misc.o	ResourcesTab.o WinTree.o Menu.o Prefs.o

# all defines
DEFS = $(KBD_DEF)

#################################
#
# X libraries, X header files and special flags for linking with X
#
##################################

#
# location of include files for the Xlib and Motif
